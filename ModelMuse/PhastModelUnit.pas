      UpdateMapping;
      for Index := 0 to Length(FColumnMapping) - 1 do
      begin
        if FColumnMapping[Index].ChildPositions[ModelIndex]
          = SelectedModel.SelectedColumn then
        begin
          CombinedDisplayColumn := Index;
          break;
        end;
      end;
    end;
