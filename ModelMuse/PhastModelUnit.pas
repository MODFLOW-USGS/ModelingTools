    atSwiObsExtInput, atSwiObsExtOutput,
    atMt3dInput, atMt3dOutput, atGeoRef, atExternal);
var
  ArchiveFiles: TXmlVerySimple;
  ANode: TXmlNode;
  FileNames: TStringList;
