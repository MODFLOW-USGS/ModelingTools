      ModflowPackages.NwtPackage.ContinueNWT := TempBool;
    end;
  end;

  if FileVersionEqualOrEarlier('2.19.1.25') then
  begin
    for ScreenObjectIndex := 0 to ScreenObjectCount -1 do
    begin
