  Mt3dActiveArray: TDataArray;
  SwrReachArray: TDataArray;
  SwrReachGroupArray: TDataArray;
  SwrRoutingTypeArray: TDataArray;
  SwrReachLengthArray: TDataArray;
  Sfr6Array: TDataArray;
  MawArray: TDataArray;
  LakeMf6Array: TDataArray;
