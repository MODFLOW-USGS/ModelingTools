      else
      begin
        if State = cbUnchecked then
        begin
          State := cbGrayed;
        end;
      end;
    end
    else
    begin
      if ScreenObjectIndex = 0 then
      begin
        State := cbUnchecked;
      end
      else
      begin
        if State = cbChecked then
        begin
          State := cbGrayed;
        end;
      end;
    end;
  end;
end;

