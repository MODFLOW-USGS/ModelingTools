      else if (Boundary = nil) or (AnotherBoundary = nil) then
      begin
        ValuesIdentical := False;
      end
      else
      begin
        ValuesIdentical := Values.IsSame(ValuesFunction(AnotherBoundary));
      end;
      if not ValuesIdentical then
      begin
        break;
