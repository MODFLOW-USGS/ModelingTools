    if ICRCC <> 0 then
    begin
      ImportSSe(Index, WtItem, ScreenObject);
      ImportSSv(Index, WtItem, ScreenObject);
    end
    else
    begin
      ImportCr(Index, WtItem, ScreenObject);
      ImportCc(Index, WtItem, ScreenObject);
