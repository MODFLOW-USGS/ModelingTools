  else if ALabel = 'NP:' then
  begin
    Readln(FImporter.FFile, AnInt);
    FImporter.UpdateProgress;
    Assert(AnInt = NPKDEP);
  end
  else if ALabel = StrVariable2DRealArray then
  begin
    ReadVariableGroundSurface;
