  FPackageIdentifiers.AddObject('RVOB:', TRivObsImporter.Create(self, Riv));
  FPackageIdentifiers.AddObject('STOB:', TStrObsImporter.Create(self, Str));
  FPackageIdentifiers.AddObject('LMT6:', nil);
