  inherited Create;
  FImportParameters := ImportParameters;
  FPointsComputed := False;
  Assert(FileExists(ImportParameters.ListFileName));
  FListFileName := ImportParameters.ListFileName;

  FPackageIdentifiers:= TStringList.Create;
