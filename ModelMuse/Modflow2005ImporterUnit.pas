    end;
  end;
  if FSfrPackage.Isfropt in [2, 3] then
  begin
    if List.Count > 1 then
    begin
      ValueArray := ScreenObject.ImportedValues.Add as TValueArrayItem;
      ValueArray.Name := 'THTS';
