  else if ID = KStage then
  begin
    if Length(FStage_Const) = 0 then
    begin
      SetLength(FStage_Const, FModel.ModflowStressPeriods.Count);
      InitializeConstArray(FStage_Const);
    end;
    ConstArray := FStage_Const;
