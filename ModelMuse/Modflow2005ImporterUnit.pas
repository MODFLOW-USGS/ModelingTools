begin
  for BoundaryIndex := 0 to ITMP - 1 do
  begin
    GhbBoundary := FStressPeriods[FCurrentStressPeriod].
      Boundaries[CurrentInstance] as TGhbLocationObject;
    Read(FImporter.FFile, LAYER);
