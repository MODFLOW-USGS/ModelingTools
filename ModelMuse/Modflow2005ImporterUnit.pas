    Read(FImporter.FFile, StartFactor);
    ChdLocation.StartFactor := StartFactor;
    Read(FImporter.FFile, EndFactor);
    ChdLocation.EndFactor := EndFactor;
    for Index := 0 to FAuxillaryVariables.Count - 1 do
